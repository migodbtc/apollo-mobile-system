{
  "expo": {
    "name": "ApolloExpo",
    "slug": "ApolloExpo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons/BW.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons/APP-COLOR-SCHEME.png",
        "backgroundColor": "#020617"
      },
      "package": "com.anonymous.ApolloExpo",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "RECORD_AUDIO"
      ]
    },
    "androidNavigationBar": {
      "backgroundColor": "#020617",
      "barStyle": "light",
      "visible": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icons/APP-COLOR-SCHEME.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#020617"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Apollo Mobile App to access your camera",
          "microphonePermission": "Allow Apollo Mobile App to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Apollo Mobile App to access your photos"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
